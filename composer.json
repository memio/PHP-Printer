    "type": "library",
    "description": "Pretty Printer for PHP Parser, powered by templates",
    "keywords": ["generator", "PHP", "templating", "code"],
    "homepage": "http://memio.github.io/PHP-Printer",
    "authors": [
        {
            "name": "Lo√Øc Faugeron",
            "email": "faugeron.loic@gmail.com",
            "homepage": "http://gnugat.github.io",
            "role": "Developer"
        }
